{
  "extends": "./eslint.js",
  "rules": {
    // disable requiring trailing commas because it might be nice to revert to
    // being JSON at some point, and I don't want to make big changes now.
    "comma-dangle": 0,
    "no-console": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-tabs":0,
    "max-len":0,
    "no-mixed-operators":0,
    "eqeqeq":0,
    "no-plusplus":0,
    "radix":0
  }
}